<mvc:View
	controllerName="sap.ui.demo.basicTemplate.controller.Home"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		id="page"
		title="Home.view.xml">
		<content>
			<f:FlexibleColumnLayout layout="TwoColumnsMidExpanded">
				<f:beginColumnPages>
					<mvc:XMLView viewName="sap.ui.demo.basicTemplate.view.Nested"></mvc:XMLView>
				</f:beginColumnPages>
				<f:midColumnPages>
					<mvc:XMLView viewName="sap.ui.demo.basicTemplate.view.NestedNested"></mvc:XMLView>
				</f:midColumnPages>
			</f:FlexibleColumnLayout>
		</content>
	</Page>
</mvc:View>